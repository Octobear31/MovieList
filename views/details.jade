- var movieDet = details;
- var x = 'https://image.tmdb.org/t/p/w500' + movieDet.poster_path
doctype html
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')
    title Document
    link(rel='stylesheet', href='style.css')
    link(href='https://fonts.googleapis.com/css?family=Luckiest+Guy|Patrick+Hand+SC|Special+Elite|Ubuntu', rel='stylesheet')
    script(src='//ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js')
  body
    .detailCell
      img(src='https://image.tmdb.org/t/p/w500/= movieDet.backdrop_path.jpg', alt='')
      div.cello
        h1= movieDet.title
        img.overview(src= x width=200 height=300)
        p= movieDet.overview
        a(href="/")
          butt.auth.padd Back to list
        
    // script connected
    script(src='js/client.js')
    // script connected
